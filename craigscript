	end)
end
coroutine.wrap(CFVZ_fake_script)()
local function RVADVV_fake_script() -- Close.SoundOnClick 
	local script = Instance.new('LocalScript', Close)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Sound:Play()
	end)
end
coroutine.wrap(RVADVV_fake_script)()
