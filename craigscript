local function HXCK_fake_script() -- CraigScript.LocalScript 
	local script = Instance.new('LocalScript', CraigScript)

	repeat wait() until game.Loaded
	
	if game.PlaceId ~= 112420803 then
		game:GetService("TeleportService"):Teleport(112420803, game:GetService("Players").LocalPlayer)
		if print() then
