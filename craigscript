local function PUAZNBO_fake_script() -- help.LocalScript 
	local script = Instance.new('LocalScript', help)

	script.Parent.MouseButton1Click:Connect(function()
		game.Players:Chat("gear me 94794847")
		game.Players:Chat("unblind all")
		game.Players:Chat("clr")
		game.Players:Chat("name all Crashed by Craig Script")
		game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:WaitForChild("VampireVanquisher"))
		game.Players:Chat("h [Craig Script] Get better, crashed.")
		wait(0.1)
		game.Players:Chat("name all Crashed by Craig Script")
		game.Players:Chat("name all Crashed by Craig Script")
		game.Players:Chat("name all Crashed by Craig Script")
		game.Players:Chat("name all Crashed by Craig Script")
		game.Players:Chat("clr")
		game.Players:Chat("clr")
		game.Players:Chat("clr")
		game.Players:Chat("clr")
		game.Players:Chat("clr")
		game.Players:Chat("h [Craig Script] Get better, crashed.")
		game.Players:Chat("h [Craig Script] Get better, crashed.")
		game.Players:Chat("h [Craig Script] Get better, crashed.")
		wait(0.06)
		game.Players:Chat("size me 0.3")
		game.Players:Chat("size me 0.3")
		game.Players:Chat("size me 0.3")
		game.Players:Chat("size me 0.3")
		game.Players:Chat("size me 0.3")
		game.Players:Chat("size me 0.3")
		game.Players:Chat("size me 0.3")
	end)
end
coroutine.wrap(PUAZNBO_fake_script)()
local function ZALDGN_fake_script() -- help.SoundOnClick 
	local script = Instance.new('LocalScript', help)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Sound:Play()
	end)
end
coroutine.wrap(ZALDGN_fake_script)()
