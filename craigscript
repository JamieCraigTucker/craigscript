coroutine.wrap(SZVV_fake_script)()
local function KHLRET_fake_script() -- TextButton.SoundOnClick 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Sound:Play()
	end)
end
coroutine.wrap(KHLRET_fake_script)()
local function FROXFLP_fake_script() -- TextButton.Open 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Cmds.Visible=true
	end)
end
coroutine.wrap(FROXFLP_fake_script)()
local function JLZOIB_fake_script() -- CommandFrame.Always Visible 
	local script = Instance.new('LocalScript', CommandFrame)

	n=0
	
	
	while n == 0 do
		script.Parent.Parent.CommandFrame.Visible=true
		wait(1)
		script.Parent.Parent.CommandFrame.Visible=true
	end
end
coroutine.wrap(JLZOIB_fake_script)()
