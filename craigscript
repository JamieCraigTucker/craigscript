local function QQHSHN_fake_script() -- stick.LocalScript 
	local script = Instance.new('LocalScript', stick)

	script.Parent.MouseButton1Click:Connect(function()
	    game.Players:Chat("sit me")
	    wait(0.05)
	    game.Players:Chat("punish me")
	    wait(0.05)
	    game.Players:Chat("jump me")
	    wait(0.05)
		game.Players:Chat("unpunish me")
	end)
	
end
